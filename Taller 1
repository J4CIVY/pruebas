        System.out.println("Ingrese por favor 2 numero");
        int variable2 = Integer.parseInt(entrada.nextLine());
        
        if (variable1 == 5){
        
        System.out.println("El Primer Numero Es El Mayor");
        
        }else{ 
        
        System.out.println("El Segundo Numero Es El Mayor");
        
        } 
            }catch(Exception e) {
